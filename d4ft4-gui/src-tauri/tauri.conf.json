{
  "build": {
    "beforeBuildCommand": "elm make src/FileTransfer.elm --output=public/main.js --optimize",
    "beforeDevCommand": "elm-live src/FileTransfer.elm --port=4000 --hot --dir=./public -- --output=public/main.js",
    "devPath": "http://localhost:4000",
    "distDir": "../public",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "d4ft4-gui",
    "version": "0.1.0"
  },
  "plugins": {
    "updater": {
      "endpoints": []
    }
  },
  "tauri": {
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "net.dacid44.d4ft4.elm-tauri",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "updater": {
        "active": false
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      },
      "android": {
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "D4FT4",
        "width": 800
      }
    ]
  }
}